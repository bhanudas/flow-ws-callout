<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Contains details for a Web Service Endpoint accessible via the Web Caller tool</description>
    <label>Web Caller Endpoint</label>
    <pluralLabel>Web Caller Endpoints</pluralLabel>
    <visibility>Public</visibility>
    <fields>
        <fullName>Address__c</fullName>
        <description>Address for End point - ie. www.endpoint.com</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Address</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Port__c</fullName>
        <description>Desired Port for communications - ie. 80 or 443</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Desired Port for communications - ie. 80 or 443</inlineHelpText>
        <label>Port</label>
        <length>6</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Protocol__c</fullName>
        <description>Selected Protocol for end point data transmission</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Selected Protocol for end point data transmission</inlineHelpText>
        <label>Protocol</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>HTTP</fullName>
                    <default>true</default>
                    <label>HTTP</label>
                </value>
                <value>
                    <fullName>HTTPS</fullName>
                    <default>false</default>
                    <label>HTTPS</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
</CustomObject>
